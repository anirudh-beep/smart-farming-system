{
  "name": "framx-react",
  "version": "2.0.0",
  "description": "FramX - Modern React Smart Farming Platform",
  "main": "server/app.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/app.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "node server/app.js",
    "install-all": "npm install && cd client && npm install",
    "setup": "npm run install-all && npm run build"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "axios": "^1.6.0",
    "csv-parser": "^3.0.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": ["framx", "react", "farming", "agriculture", "soil-analysis", "crop-recommendation"],
  "author": "FramX Team",
  "license": "MIT"
}